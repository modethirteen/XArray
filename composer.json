{
  "name": "modethirteen/xarray",
  "description": "A utility for traversing PHP arrays with an XPath-like syntax",
  "type": "library",
  "license": "Apache-2.0",
  "authors" : [
    {
      "name": "James Andrew Vaughn",
      "email": "jamesvaughn@modethirteen.com",
      "role": "maintainer"
    }
  ],
  "support": {
    "issues": "https://github.com/modethirteen/XArray/issues"
  },
  "require": {
    "php": "^7.2.0",
    "ext-json": "*",
    "modethirteen/type-ex": "1.0.0"
  },
  "require-dev": {
    "phpstan/phpstan": "~0.12.0",
    "phpunit/phpunit": "~7.4.0"
  },
  "autoload": {
    "psr-4": {
      "modethirteen\\XArray\\": ["src/"],
      "MindTouch\\XArray\\": ["compatibility/"]
    }
  },
  "autoload-dev": {
    "psr-4": {
      "modethirteen\\XArray\\Tests\\": ["tests/"]
    }
  }
}
